keywords<-function (d) {
a<-list( "garden",  "dishwasher", "patio",
"backyard", "gated community", "courtyard", "patios",
"balconies", "balcony", "upgraded","cable included",
"upgraded", "luxury", "balconies", "backyard", "fencing",
"dining room", "walkin", "fireplace", "renovated", "remodeled",
"tile", "carpet", "pavillion", "roof deck","carpeting",
"resort", "fenced", "house", "roofdeck","internet included",
"granite", "gym", "business center", "pool", "townhouse",
"vaulted", "duplex", "stone", "utilities included", "back yard",
"bungalow", "office", "den", "sunroom", "walk in", "sun terrace",
"laundry", "parking", "jacuzzi", "french doors","covered parking",
"screen porch", "corporate", "fitness","walk-in", "roofterrace",
"fireplace", "marble", "crown molding", "storage", "bills included",
"controlled", "terrace", "shuttle",  "corner", "fence", "highrise",
"updated", "furnished", "urban", "penthouse", "elevator","roof terrace",
"sun room", "screened porch", "yard", "porch", "court yard","sundeck","new construction",
"executive","garage","sun deck","hot tub","historic", "street parking", "parking on the street")
for(z in a){
d[[z]]<-grepl(paste("\\<",z,"\\>",sep=""), d$text,ignore.case=TRUE)
d[[z]]<-ifelse(d[[z]]=="TRUE",1,0)
}
d$screenporch<-with(d,ifelse((`screen porch`==1 |`screened porch`==1) & porch==1,1,0))
d$gym<-with(d, ifelse(gym==1 | fitness==1,1,0))
d$utilities<-with(d, ifelse(`utilities included`==1 | `bills included` |`cable included` | `internet included`,1,0))
d$businesscenter<-with(d, ifelse(`business center`==1,1,0))
d$frenchdoors<-with(d, ifelse(`french doors`==1,1,0))
d$courtyard<-with(d, ifelse((`court yard`==1) | courtyard==1,1,0))
d$sundeck<-with(d, ifelse((`sun deck`==1) | sundeck==1,1,0))
d$roofdeck<-with(d, ifelse(`roof deck`==1 | roofdeck, 1,0))
d$roofterrace<-with(d, ifelse(`roof terrace`==1 | roofterrace, 1,0))
d$diningroom<-with(d, ifelse(`dining room`==1, 1,0))
d$crownmolding<-with(d, ifelse(`crown molding`==1, 1,0))
d$newconstruction<-with(d, ifelse(`new construction`==1, 1,0))
d$deck<-with(d, ifelse(deck==1 & `sun deck`==0 & `roof deck`==0, 1,0))
d$terrace<-with(d, ifelse(terrace==1 & `roof terrace`==0 & `sun terrace`==0, 1,0))
d$porch<-with(d, ifelse(porch==1 & screenporch==0, 1,0))
d$yard<-with(d, ifelse((yard==1 | `back yard`==1 | backyard==1) & courtyard==0, 1,0))
d$balcony<-with(d, ifelse(balcony==1 | balconies==1, 1,0))
d$patio<-with(d, ifelse(patio==1 | patios==0, 1,0))
d$gated<-with(d, ifelse(`gated community`==0, 1,0))
d$parking<-with(d, ifelse(`covered parking`==0 & parking==1 & `parking on the street` & `street parking`, 1,0))
d$covered<-with(d, ifelse(`covered parking`==1, 1,0))
d$fenced<-with(d, ifelse(fencing==1 | fenced==1 | fence==1, 1,0))
d$carpet<-with(d, ifelse(carpet==1 | carpeting==1 , 1,0))
d$renovated<-with(d, ifelse(remodeled==1 | renovated==1 , 1,0))
d$updated<-with(d, ifelse(updated==1 | upgraded==1 , 1,0))
d$walkin<-with(d, ifelse(walkin==1 | `walk in`==1 | `walk-in`==1 , 1,0))
d$hottub<-with(d, ifelse(`hot tub`==1 , 1,0))
d$den<-with(d, ifelse(office==1 | den==1 , 1,0))
d$corporate<-with(d, ifelse(corporate==1 | executive==1 , 1,0))
return(d)
}
clean<-function(x){
x$zip<-as.numeric(as.character(x$zip))
x$listdate<-as.Date(paste(x$listing_month, x$listing_day, x$listing_year,sep = "/", collapse = NULL),"%m/%d/%Y")
x$beds<-round(x$beds)
x$baths<-ifelse(x$baths<1 | x$baths>4 | (x$beds==1 & x$baths>1.5)|( x$beds==0 & x$bath>1)|
(x$beds>=1 & x$baths>(x$beds+1)), NA, (round(x$baths/.5))*.5)
x<-subset(x,(zip>=19090 & zip<=19154 ) & beds<=5)
x<-keywords(x)
return(x)
}
